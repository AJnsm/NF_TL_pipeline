params{
    h5file = "${projectDir}/cellData/1M_neurons_20k.h5"
    nGenes = 15
    nCells = 100
    report_dir = "${launchDir}/report"

}



profiles {
    eddie {
        includeConfig 'eddie.config'
    }

    eddieTest {
        includeConfig 'eddie.config'
    }

}


report {
    enabled = true
    file = "${params.report_dir}/report.html"
}

timeline {
    enabled = true
    file = "${params.report_dir}/timeline.html"
}

trace {
    enabled = true
    file = "${params.report_dir}/trace.txt"
}

dag {
    enabled = true
    file = "${params.eport_dir}/pipeline.svg"
}
